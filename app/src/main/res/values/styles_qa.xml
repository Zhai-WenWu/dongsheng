<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--举报页面-->
    <style name="report_desc">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/dp_11</item>
        <item name="android:paddingBottom">@dimen/dp_9</item>
        <item name="android:paddingLeft">@dimen/dp_20</item>
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:textColor">#999999</item>
    </style>
    <style name="report_commit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_46</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_marginTop">@dimen/dp_20</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:layout_marginBottom">@dimen/dp_15</item>
        <item name="android:background">@drawable/bg_btn_report_quan</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="report_item_desc">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/sp_15</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginLeft">@dimen/dp_20</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <style name="report_line_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">#edeaea</item>
    </style>
    <style name="switch_btn_style">
        <item name="android:layout_width">@dimen/dp_40</item>
        <item name="android:layout_height">@dimen/dp_30</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/dp_20</item>
        <item name="android:src">@drawable/switch_btn_bgselector</item>
    </style>
</resources>