<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RichText">
        <attr name="bulletColor" format="reference|color" />
        <attr name="bulletRadius" format="reference|dimension" />
        <attr name="bulletGapWidth" format="reference|dimension" />
        <attr name="historyEnable" format="reference|boolean" />
        <attr name="historySize" format="reference|integer" />
        <attr name="linkColor" format="reference|color" />
        <attr name="linkUnderline" format="reference|boolean" />
        <attr name="quoteColor" format="reference|color" />
        <attr name="quoteStripeWidth" format="reference|dimension" />
        <attr name="quoteCapWidth" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="OverScrollView">
        <attr name="defaultPaddingTop" format="dimension"></attr>
        <attr name="defaultPaddingBottom" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="StickHeaderLayout">
        <attr name="scrollViewId" format="reference" />
    </declare-styleable>
    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsIndicatorPaddingLeftRight" format="dimension" />
        <attr name="pstsIndicatorWidthFollowText" format="boolean" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsDividerWidth" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsPaddingMiddle" format="boolean" />
        <attr name="pstsTabColumn" format="integer" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsTabTextSize" format="dimension" />
        <attr name="pstsTabSelectedTextSize" format="dimension" />
        <attr name="pstsTabTextColor" format="reference" />
        <attr name="pstsTabTextStyle" format="reference">
            <flag name="normal" value="0x0" />
            <flag name="bold" value="0x1" />
            <flag name="italic" value="0x2" />
        </attr>
        <attr name="pstsTabTextAllCaps" format="boolean" />
        <attr name="pstsTabTextAlpha" format="integer" />
        <attr name="pstsTabTextFontFamily" format="string" />
    </declare-styleable>
    <!-- 首页头部title -->
    <declare-styleable name="HomeTitle">
        <attr name="htTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="TagTextView">
        <attr name="radius" format="dimension"/>
        <attr name="topLeftRadius" format="dimension"/>
        <attr name="topRightRadius" format="dimension"/>
        <attr name="bottomLeftRadius" format="dimension"/>
        <attr name="bottomRightRadius" format="dimension"/>
        <attr name="sideWidth" format="dimension"/>
        <attr name="sideDashWidth" format="dimension"/>
        <attr name="sideDashGap" format="dimension"/>
        <attr name="sideColor" format="color"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="MultiTagView">
        <attr name="parentMargin" format="dimension"/>
        <attr name="buttonAddColor" format="color"/>
        <attr name="buttonAddClickColor" format="color"/>
        <attr name="tagTextColor" format="color"/>
        <attr name="tagClickColor" format="color"/>
        <attr name="tagPadding" format="dimension"/>
        <attr name="tagMargin" format="dimension"/>
        <attr name="tagPaddingTop" format="dimension"/>
        <attr name="tagPaddingBottom" format="dimension"/>
        <attr name="tagPaddingRight" format="dimension"/>
        <attr name="tagPaddingLeft" format="dimension"/>
        <attr name="tagMarginTop" format="dimension"/>
        <attr name="tagHeight" format="dimension"/>
        <attr name="deleteDrawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TextViewLimitLine">
        <attr name="initLine" format="integer"/>
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="absoluteMinValue" format="integer|float"/>
        <attr name="absoluteMaxValue" format="integer|float"/>
        <attr name="singleThumb" format="boolean"/>
    </declare-styleable>

</resources>